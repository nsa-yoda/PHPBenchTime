{
    "name": "nsa-yoda/phpbenchtime",
    "type": "library",
    "description": "A lightweight benchmark timer and lap profiler for PHP.",
    "keywords": [
        "benchmark",
        "timer",
        "benchtime",
        "profiler",
        "stopwatch",
        "performance"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Sanchez",
            "email": "juan.sanchez@juanleonardosanchez.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "test": "phpunit --no-coverage"
    }
    "autoload": {
        "psr-4": {
            "PHPBenchTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPBenchTime\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}